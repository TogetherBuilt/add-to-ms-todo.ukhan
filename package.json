    "url-parse": "^1.5.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^26.6.3",
