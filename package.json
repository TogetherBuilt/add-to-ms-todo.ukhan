    "bestzip": "^2.2.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.23.4",
    "fetch-mock-jest": "^1.5.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.11.3",
